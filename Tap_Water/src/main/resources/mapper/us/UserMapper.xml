<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zhibang.mapper.us.UserMapper">

    <insert id="addUser">
        INSERT INTO `tap_water`.`us_user`
          (`UserNo`, `UserName`, `CreateDate`, `Address`, `Phone`, `UserType`)
        VALUES
          (#{userNo}, #{userName}, NOW(), #{address}, #{phone}, #{userType}) ;
    </insert>

</mapper>